{
  "name": "bulletin-board-system-frontend",
  "version": "1.5.3",
  "description": "A generic frontend for Single-Page Web Application with a static server.",
  "main": "./src/lib/server/app.js",
  "config": {
    "ip": "0.0.0.0",
    "port": 8088
  },
  "scripts": {
    "-------------- EXPOSED --------------": "",
    "start": "node --stack-size=65500 src/lib/server/app.js",
    "start-prod": "NODE_ENV=production node --stack-size=65500 dist/lib/server/app.js",
    "lint": "npm run eslint",
    "test": "jest",
    "integration-test": "",
    "coverage:unit-test": "",
    "coverage:integration-test": "",
    "prepush": "npm run lint",
    "postinstall": "grunt postinstall",
    "build": "babel --source-maps --copy-files src/lib/ --out-dir dist/lib/",
    "-------------- INTERNAL -------------": "",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' --ext .js,.jsx src/lib/client/src/ src/lib/server/ src/spec/",
    "-------------- LOCAL DEV ------------": "",
    "local-unit-test": "",
    "local-integration-test": "",
    "coverage:report": "",
    "coverage:html-report": "",
    "dev:start": "grunt dev",
    "dev:prod-preview": "grunt prod:preview",
    "dev:lint": "grunt lint --force",
    "dev:test": "grunt test --force",
    "debug": "node --debug --debug-brk src/lib/server/app.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src/spec/unit"
    ],
    "testRegex": "src/spec/unit/.*\\.spec\\.(jsx|js)$",
    "preprocessorIgnorePatterns": [
      "/node_modules/"
    ],
    "automock": true,
    "unmockedModulePathPatterns": [
      "node_modules/sinon",
      "node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/enzyme",
      "node_modules/randomstring"
    ],
    "verbose": true
  },
  "engines": {
    "node": "6.1.0",
    "npm": ">=3.10.9"
  },
  "dependencies": {
    "autoprefixer": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "cssnano": "^3.8.0",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "flux": "^3.1.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-env": "^0.4.4",
    "grunt-eslint": "^19.0.0",
    "grunt-express-server": "^0.5.3",
    "grunt-jest": "^0.3.0",
    "grunt-open": "^0.2.3",
    "grunt-postcss": "^0.8.0",
    "jade": "^1.11.0",
    "jit-grunt": "^0.10.0",
    "keymirror": "^0.1.1",
    "method-override": "^2.3.6",
    "morgan": "^1.7.0",
    "node-uuid": "^1.4.7",
    "pixrem": "^3.0.2",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "request": "^2.79.0",
    "serve-favicon": "^2.3.0",
    "time-grunt": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^16.0.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "husky": "^0.11.9",
    "jest-cli": "^16.0.2"
  },
  "author": "Marcus Hsu",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:kiddkevin00/bulletin-board-system-frontend.git"
  }
}
