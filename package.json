{
  "name": "sugarpost-frontend-web-core",
  "version": "3.1.0",
  "description": "A generic frontend for Single-Page Web Application with a static server.",
  "main": "./src/lib/server/app.js",
  "bin": {
    "update-test-coverage": "./bin/update-test-coverage.js",
    "version-bump": "./bin/version-bump.js",
    "restrict-branches-no-push": "./bin/restrict-branches-no-push.sh"
  },
  "config": {
    "ip": "0.0.0.0",
    "port": 8088
  },
  "nyc": {
    "statements": 0,
    "branches": 0,
    "functions": 0,
    "lines": 0,
    "exclude": [
      "**/*.spec.js"
    ],
    "include": [
      "src/lib/**/*.js"
    ],
    "report-dir": "./spec/coverage"
  },
  "scripts": {
    "-------------- EXPOSED --------------": "",
    "start": "node --stack-size=65500 src/lib/server/app.js",
    "start-prod": "node --stack-size=65500 dist/lib/server/app.js",
    "lint": "npm run eslint",
    "unit-test": "cat src/spec/support/run_all_the_tests.txt; npm run mocha-unit-test",
    "integration-test": "",
    "precommit": "npm run lint && npm run coverage:unit-test",
    "prepush": "npm run branch-check && npm run lint && npm run coverage:unit-test && npm run coverage:update-thresholds",
    "postinstall": "grunt postinstall",
    "version:bump": "npm run version:instruction && ./bin/version-bump.js",
    "build": "NODE_ENV=webpack webpack",
    "-------------- INTERNAL -------------": "",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' --ext .js,.jsx src/lib/client/src/ src/lib/server/ src/spec/ bin/",
    "jest-unit-test": "jest",
    "mocha-unit-test": "multi='tap=- xunit=spec/reports/xunit.xml' mocha --opts mocha.opts --reporter mocha-multi | faucet",
    "coverage:unit-test": "nyc --check-coverage --all --reporter cobertura --reporter html mocha --opts mocha.opts",
    "coverage:integration-test": "",
    "coverage:update-thresholds": "echo 'disable `coverage:update-thresholds` for now' || ./bin/update-test-coverage.js",
    "branch-check": "./bin/restrict-branches-to-push.sh",
    "version:instruction": "echo 'Please specify full version number after `npm run version:bump`.  E.g. $ npm run version:bump 0.0.1'",
    "-------------- LOCAL DEV ------------": "",
    "local-unit-test": "mocha --opts mocha.opts --bail",
    "local-integration-test": "",
    "coverage:report": "nyc report",
    "coverage:html-report": "open spec/coverage/index.html",
    "build:bebel": "babel --source-maps --copy-files src/lib/ --out-dir dist/lib/",
    "dev:start": "grunt dev",
    "dev:prod-preview": "grunt prod:preview",
    "dev:lint": "grunt lint --force",
    "dev:test": "grunt test --force",
    "debug": "node --debug --debug-brk src/lib/server/app.js"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/unit_jest/**/*.(spec|test).js?(x)"
    ]
  },
  "engines": {
    "node": "6.1.0",
    "npm": ">=3.10.9"
  },
  "dependencies": {
    "autoprefixer": "6.7.7",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babelify": "7.3.0",
    "body-parser": "1.17.1",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.3",
    "coupon-code": "0.4.5",
    "cssnano": "3.10.0",
    "errorhandler": "1.5.0",
    "express": "4.15.2",
    "flux": "3.1.2",
    "grunt": "1.0.1",
    "grunt-babel": "6.0.0",
    "grunt-browserify": "5.0.0",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-uglify": "2.3.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-env": "0.4.4",
    "grunt-eslint": "19.0.0",
    "grunt-express-server": "0.5.3",
    "grunt-open": "0.2.3",
    "grunt-postcss": "0.8.0",
    "jade": "1.11.0",
    "jit-grunt": "0.10.0",
    "keymirror": "0.1.1",
    "method-override": "2.3.8",
    "morgan": "1.8.1",
    "node-uuid": "1.4.8",
    "pixrem": "3.0.2",
    "promise-polyfill": "6.0.2",
    "prop-types": "15.5.8",
    "react": "15.4.2",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.4.2",
    "react-ga": "2.2.0",
    "react-redux": "5.0.4",
    "react-router": "3.0.3",
    "react-router-bootstrap": "0.23.1",
    "react-router-redux": "4.0.8",
    "react-scroll": "1.4.8",
    "react-share": "1.13.0",
    "react-stripe-checkout": "2.2.5",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "serve-favicon": "2.4.2",
    "time-grunt": "1.4.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.1",
    "babel-jest": "19.0.0",
    "babel-loader": "7.0.0",
    "babel-preset-env": "1.4.0",
    "babel-register": "6.24.0",
    "bluebird": "3.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-shallow-deep-equal": "1.4.6",
    "enzyme": "2.8.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-config-standard-react": "5.0.0",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.7.1",
    "eslint-plugin-chai-expect": "1.1.1",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.10.3",
    "faucet": "0.0.1",
    "husky": "0.11.9",
    "jest": "19.0.2",
    "jsdom": "9.12.0",
    "mocha": "3.2.0",
    "mocha-multi": "0.10.0",
    "nyc": "8.4.0",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.5.4",
    "sinon": "1.17.7",
    "sinon-as-promised": "4.0.3",
    "sinon-chai": "2.9.0",
    "webpack": "2.4.1"
  },
  "author": "Marcus Hsu",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:kiddkevin00/bulletin-board-system-frontend.git"
  }
}
