{
  "env": {
    "jasmine": false,
    "mocha": true
  },
  "globals": {
    "expect": true,
    "spy": true,
    "stub": true
  },
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": 0,
    "keyword-spacing": [2, {
      "overrides": {
        "throw": { "after": false, "before": true }
      }
    }],
    "spaced-comment": 0,
    "padded-blocks": 0,
    "no-unused-expressions": 1,
    "new-cap": [2, { "capIsNew": false }],
    "react/jsx-filename-extension": 0,
    "react/jsx-curly-spacing": [2, "always"],
    "import/first": 0
  }
}
