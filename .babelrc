{
  "env": {
    "development": {
      "presets": ["react", "es2015"],
      "plugins": ["add-module-exports"],
      "sourceMap": true
    },
    "test": {
      "presets": ["react", "es2015"],
      "plugins": ["add-module-exports"],
      "sourceMap": "both"
    },
    "production": {
      "presets": ["react", "es2015"],
      "plugins": ["add-module-exports"],
      "sourceMap": "both"
    },
    "webpack": {
      "presets": [
        "react",
        ["es2015", { "modules": false, "loose": true }]
      ]
    }
  }
}
